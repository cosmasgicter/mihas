    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^6.0.1",
    "typescript": "^5.5.3",
    "vite": "^7.1.5",
    "vite-plugin-pwa": "^1.0.3",
    "web-vitals": "^5.1.0",
